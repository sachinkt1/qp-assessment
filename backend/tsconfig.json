{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "moduleResolution": "node",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "allowSyntheticDefaultImports": true,
      "strict": true,
      "outDir": "./dist"   // Add this line to specify the output directory
    },
    "include": ["server.ts"]
  }
  